{
  "name": "hplife",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android  --port=8082",
    "ios": "react-native run-ios --simulator=\"iPhone 15\" --port=8082",
    "ios:release": "react-native run-ios --mode Release --simulator=\"iPhone 15\"  --port=8082",
    "lint": "eslint .",
    "start": "react-native start --port=8082",
    "test": "jest",
    "clean": "watchman watch-del-all && npm cache clean --force && npm cache verify && rm -fr $TMPDIR/react-* && react-native start --reset-cache",
    "pods": "cd ios && pod install && cd ..",
    "postinstall": "patch-package && yarn pods",
    "audit-fix": "npx yarn-audit-fix",
    "outdated": "npm outdated",
    "android:bundleRelease": "cd android && ./gradlew bundleRelease",
    "android:apkRelease": "cd android && ./gradlew assembleRelease",
    "firebase:ipa": "bundle exec fastlane buildIOSOnly",
    "firebase:apk": "bundle exec fastlane buildAndroidOnly",
    "bump": "bundle exec fastlane bumpVersion"
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.6.10",
    "@notifee/react-native": "^7.8.2",
    "@react-native-async-storage/async-storage": "^2.0.0",
